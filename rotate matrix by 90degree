  first move must be 
  
   make the 0,0 eelemnt matches with expected output 0,0 element ;
    
    after that rotate;
  
  int n=matrix.size();
        int m=matrix[0].size();
        
        
          reverse(matrix.begin(),matrix.end());
        
          for(int i=0;i<n;i++)
          {
              for(int j=i;j<m;j++)
              {
                  swap(matrix[i][j],matrix[j][i]);
              }
          }
        
